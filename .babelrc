{
  "presets": ["next/babel"],
  "env": {
    // production에서 console.log 보이지 않도록 설정
    "production": { "plugins": ["batransform-remove-console"] }
  },
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ]
  ]
}
